{
  "name": "MiniTwit Dev",
  "dockerComposeFile": "../docker-compose.dev.yml",
  "service": "app",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "ms-azuretools.vscode-docker"
      ]
    }
  },
  "postCreateCommand": "mvn dependency:go-offline && mvn compile exec:java -Dexec.mainClass=zerodowntime.App -Dexec.args='init'"
}